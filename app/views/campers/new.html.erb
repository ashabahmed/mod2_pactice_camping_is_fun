<h1>New Camper</h1>
<hr>
<% if flash[:error_message] %>
    <% flash[:error_message].each do |error| %>
        <%= error %>
    <% end %>
<% end %>
<br>
<%= form_for @camper do |f|%>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :age %>
<%= f.number_field :age %>
<%= f.submit %>
<%end%>